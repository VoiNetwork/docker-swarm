#!/bin/bash

source "${HOME}/voi/bin/.helpers/util.sh"

function validate_supported_node_type() {
  if [[ ${VOINETWORK_PROFILE} != "participation" && ${VOINETWORK_PROFILE} != "relay" ]]; then
    util_abort "This operation is only supported for participation and relay nodes. Exiting the program."
  fi
}

validate_supported_node_type
util_validate_running_container

# shellcheck disable=SC2068
docker exec -it "${CONTAINER_ID}" /node/bin/goal -d /algod/data $@
